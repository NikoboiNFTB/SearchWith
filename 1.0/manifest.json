{
  "manifest_version": 2,
  "name": "Search With",
  "version": "1.0",
  "description": "Adds search options to the context menu for highlighted text.",
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png"
  }
}